<UserControl
    x:Class="WizMes_WellMade.Win_dvl_MoldRegularInspectBasis_U"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dgx="clr-namespace:DataGridExtensions;assembly=DataGridExtensions"
    xmlns:local="clr-namespace:WizMes_WellMade"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="620"
    d:DesignWidth="820"
    Loaded="UserControl_Loaded"
    mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="88*" />
            <RowDefinition Height="532*" />
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="{DynamicResource WizMes_WellMade}">
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="100*" />
                <ColumnDefinition Width="180*" />
                <ColumnDefinition Width="200*" />
                <ColumnDefinition Width="80*" />
                <ColumnDefinition Width="250*" />
            </Grid.ColumnDefinitions>
            <Grid
                x:Name="grdSrh1"
                Grid.Row="0"
                Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="3*" />
                    <ColumnDefinition Width="4*" />
                </Grid.ColumnDefinitions>
                <Label
                    x:Name="lblMoldSrh"
                    Grid.Column="0"
                    Margin="1"
                    MouseLeftButtonUp="lblMoldSrh_MouseLeftButtonUp">
                    <StackPanel VerticalAlignment="Center" Orientation="Horizontal">
                        <CheckBox
                            x:Name="chkMoldNoSrh"
                            VerticalAlignment="Center"
                            Checked="chkMoldSrh_Checked"
                            Unchecked="chkMoldSrh_Unchecked" />
                        <TextBlock VerticalAlignment="Center" Text="금형명" />
                    </StackPanel>
                </Label>
                <TextBox
                    x:Name="txtMoldNoSrh"
                    Grid.Column="1"
                    Margin="1"
                    IsEnabled="False" />
            </Grid>

            <Grid
                x:Name="grdSrh2"
                Grid.Row="1"
                Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="3*" />
                    <ColumnDefinition Width="4*" />
                </Grid.ColumnDefinitions>
                <Label
                    x:Name="lblArticleSrh"
                    Grid.Column="0"
                    Margin="1"
                    MouseLeftButtonUp="lblArticleSrh_MouseLeftButtonUp">
                    <StackPanel VerticalAlignment="Center" Orientation="Horizontal">
                        <CheckBox
                            x:Name="chkArticleSrh"
                            VerticalAlignment="Center"
                            Checked="chkArticleSrh_Checked"
                            Unchecked="chkArticleSrh_Unchecked" />
                        <TextBlock VerticalAlignment="Center" Text="품명" />
                    </StackPanel>
                </Label>
                <TextBox
                    x:Name="txtArticleSrh"
                    Grid.Column="1"
                    Margin="1"
                    IsEnabled="False" />
            </Grid>

            <Grid Grid.RowSpan="4" Grid.Column="4">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Button
                        x:Name="btnCopy"
                        Grid.Column="0"
                        Margin="1"
                        Click="btnCopy_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image
                                VerticalAlignment="Center"
                                Source="{DynamicResource btiInsert}"
                                Style="{DynamicResource imgSize}" />
                            <TextBlock VerticalAlignment="Center" Text="유지추가" />
                        </StackPanel>
                    </Button>
                    <Button
                        x:Name="btnAdd"
                        Grid.Column="1"
                        Margin="1"
                        Click="btnAdd_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image
                                VerticalAlignment="Center"
                                Source="{DynamicResource btiInsert}"
                                Style="{DynamicResource imgSize}" />
                            <TextBlock VerticalAlignment="Center" Text="{Binding Source={StaticResource tblInsert}, Mode=OneWay, Path=Text}" />
                        </StackPanel>
                    </Button>
                    <Button
                        x:Name="btnUpdate"
                        Grid.Column="2"
                        Margin="1"
                        Click="btnUpdate_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image
                                VerticalAlignment="Center"
                                Source="{DynamicResource btiUpdate}"
                                Style="{DynamicResource imgSize}" />
                            <TextBlock VerticalAlignment="Center" Text="{Binding Source={StaticResource tblUpdate}, Mode=OneWay, Path=Text}" />
                        </StackPanel>
                    </Button>
                    <Button
                        x:Name="btnDelete"
                        Grid.Column="3"
                        Margin="1"
                        Click="btnDelete_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image
                                VerticalAlignment="Center"
                                Source="{DynamicResource btiDelete}"
                                Style="{DynamicResource imgSize}" />
                            <TextBlock VerticalAlignment="Center" Text="{Binding Source={StaticResource tblDelete}, Mode=OneWay, Path=Text}" />
                        </StackPanel>
                    </Button>
                    <Button
                        x:Name="btnClose"
                        Grid.Column="4"
                        Margin="1"
                        Click="btnClose_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image
                                VerticalAlignment="Center"
                                Source="{DynamicResource btiClose}"
                                Style="{DynamicResource imgSize}" />
                            <TextBlock VerticalAlignment="Center" Text="{Binding Source={StaticResource tblClose}, Mode=OneWay, Path=Text}" />
                        </StackPanel>
                    </Button>
                </Grid>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Button
                        x:Name="btnSearch"
                        Grid.Column="1"
                        Margin="1"
                        Click="btnSearch_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image
                                VerticalAlignment="Center"
                                Source="{DynamicResource btiSearch}"
                                Style="{DynamicResource imgSize}" />
                            <TextBlock VerticalAlignment="Center" Text="{Binding Source={StaticResource tblSearch}, Mode=OneWay, Path=Text}" />
                        </StackPanel>
                    </Button>
                    <Button
                        x:Name="btnSave"
                        Grid.Column="2"
                        Margin="1"
                        Click="btnSave_Click"
                        Visibility="Hidden">
                        <StackPanel Orientation="Horizontal">
                            <Image
                                VerticalAlignment="Center"
                                Source="{DynamicResource btiSave}"
                                Style="{DynamicResource imgSize}" />
                            <TextBlock VerticalAlignment="Center" Text="{Binding Source={StaticResource tblSave}, Mode=OneWay, Path=Text}" />
                        </StackPanel>
                    </Button>
                    <Button
                        x:Name="btnCancel"
                        Grid.Column="3"
                        Margin="1"
                        Click="btnCancel_Click"
                        Visibility="Hidden">
                        <StackPanel Orientation="Horizontal">
                            <Image
                                VerticalAlignment="Center"
                                Source="{DynamicResource btiCancle}"
                                Style="{DynamicResource imgSize}" />
                            <TextBlock VerticalAlignment="Center" Text="{Binding Source={StaticResource tblCancle}, Mode=OneWay, Path=Text}" />
                        </StackPanel>
                    </Button>
                    <Button
                        x:Name="btnExcel"
                        Grid.Column="4"
                        Margin="1"
                        Click="btnExcel_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image
                                VerticalAlignment="Center"
                                Source="{DynamicResource btiExcel}"
                                Style="{DynamicResource imgSize}" />
                            <TextBlock VerticalAlignment="Center" Text="{Binding Source={StaticResource tblExcel}, Mode=OneWay, Path=Text}" />
                        </StackPanel>
                    </Button>
                </Grid>
            </Grid>
        </Grid>
        <Grid Grid.Row="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition Height="auto" />
            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0">
                    <DataGrid
                        x:Name="dgdMain"
                        ColumnHeaderHeight="30"
                        SelectionChanged="dgdMain_SelectionChanged"
                        Style="{DynamicResource DataGridSet1}">
                        <DataGrid.Columns>
                            <!--<DataGridTemplateColumn Header="순번" Width="auto">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Num}" TextAlignment="Center" Padding="3 0" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>-->

                            <DataGridTemplateColumn
                                Width="auto"
                                Header="금형 ID"
                                Visibility="Hidden">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock
                                            Padding="3,0"
                                            Text="{Binding MoldID}"
                                            TextAlignment="Center" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn Width="auto" Header="금형명">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock
                                            Padding="3,0"
                                            Text="{Binding MoldNo}"
                                            TextAlignment="Center" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn Width="auto" Header="품명">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock
                                            Padding="3,0"
                                            Text="{Binding Article}"
                                            TextAlignment="Center" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn Width="auto" Header="개정일자">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock
                                            Padding="3,0"
                                            Text="{Binding MoldInspectBasisDate}"
                                            TextAlignment="Center" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn Width="auto" Header="개정내용">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock
                                            Padding="5,0,0,0"
                                            Text="{Binding MoldInspectContent}"
                                            TextAlignment="Left" />
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>

                <GridSplitter
                    Grid.Column="1"
                    Width="5"
                    ResizeBehavior="PreviousAndNext" />

                <Grid Grid.Column="2">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0.15*" />
                        <RowDefinition Height="1.25*" />
                        <RowDefinition Height="1.8*" />
                    </Grid.RowDefinitions>
                    <!--  0 Row 메세지  -->
                    <Grid Grid.Row="0" Grid.RowSpan="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="6*" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Label
                            x:Name="lblMsg"
                            Grid.Column="1"
                            Margin="2"
                            HorizontalContentAlignment="Center"
                            VerticalContentAlignment="Center"
                            Background="Yellow"
                            Foreground="Red"
                            Visibility="Hidden">
                            <TextBlock
                                x:Name="tbkMsg"
                                Margin="2"
                                FontWeight="Bold"
                                Text="자료 입력 중" />
                        </Label>
                    </Grid>

                    <!--  1 Row - 작성 부분  -->
                    <Grid
                        x:Name="grdInput"
                        Grid.Row="1"
                        IsHitTestVisible="False">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1.1*" />
                            <ColumnDefinition Width="2*" />
                            <ColumnDefinition Width="0.3*" />
                            <ColumnDefinition Width="2*" />
                        </Grid.ColumnDefinitions>
                        <!--  기준번호  -->
                        <Label
                            Margin="1"
                            HorizontalContentAlignment="Left"
                            VerticalContentAlignment="Center"
                            Content="기준번호" />
                        <TextBox
                            x:Name="txtMoldInspectBasisID"
                            Grid.Row="0"
                            Grid.Column="1"
                            Margin="1"
                            VerticalContentAlignment="Center"
                            Background="#c2fdc3"
                            IsHitTestVisible="False"
                            Text="{Binding MoldInspectBasisID}" />
                        <!--  점검주기구분  -->
                        <Label
                            Grid.Row="1"
                            Margin="1"
                            HorizontalContentAlignment="Left"
                            VerticalContentAlignment="Center"
                            Content="점검주기구분" />
                        <ComboBox
                            x:Name="cboInspectCycle"
                            Grid.Row="1"
                            Grid.Column="1"
                            Margin="1"
                            VerticalContentAlignment="Center"
                            SelectedValue="{Binding InspectCycle}"
                            Style="{DynamicResource ComboBoxInputString}" />

                        <!--  금형 Lot No  -->
                        <Label
                            Grid.Row="2"
                            Grid.Column="0"
                            Margin="1"
                            HorizontalContentAlignment="Left"
                            VerticalContentAlignment="Center"
                            Content="금형명" />
                        <TextBox
                            x:Name="txtMoldID"
                            Grid.Row="2"
                            Grid.Column="1"
                            Margin="1"
                            VerticalContentAlignment="Center"
                            Background="#fff2d2"
                            KeyDown="txtMoldID_KeyDown"
                            Tag="{Binding MoldID, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                            Text="{Binding MoldNo, UpdateSourceTrigger=PropertyChanged}" />
                        <Button
                            x:Name="btnPfMoldID"
                            Grid.Row="2"
                            Grid.Column="2"
                            Margin="1"
                            Click="btnPfMoldID_Click">
                            <Image Source="{DynamicResource btiPlusFind}" />
                        </Button>

                        <!--  개정일자  -->
                        <Label
                            Grid.Row="3"
                            Grid.Column="0"
                            Margin="1"
                            HorizontalContentAlignment="Left"
                            VerticalContentAlignment="Center"
                            MouseLeftButtonUp="lblMoldInspectBasisDate_MouseLeftButtonUp">
                            <StackPanel Orientation="Horizontal">
                                <CheckBox
                                    x:Name="chkMoldInspectBasisDate"
                                    VerticalAlignment="Center"
                                    IsChecked="True" />
                                <TextBlock VerticalAlignment="Center" Text="개정일자" />
                            </StackPanel>
                        </Label>
                        <DatePicker
                            x:Name="dtpMoldInspectBasisDate"
                            Grid.Row="3"
                            Grid.Column="1"
                            Margin="1"
                            Padding="0"
                            VerticalContentAlignment="Center"
                            SelectedDate="{Binding MoldInspectBasisDate}" />

                        <!--  품번  -->
                        <Label
                            Grid.Row="4"
                            Grid.Column="0"
                            Margin="1"
                            HorizontalContentAlignment="Left"
                            VerticalContentAlignment="Center"
                            Content=" 품    번" />
                        <TextBox
                            x:Name="txtBuyerArticleNo"
                            Grid.Row="4"
                            Grid.Column="1"
                            Margin="1"
                            VerticalContentAlignment="Center"
                            Style="{DynamicResource essentialTextBox}"
                            Tag="{Binding ArticleID}"
                            Text="{Binding BuyerArticleNo}" />

                        <!--  품명  -->
                        <Label
                            Grid.Row="5"
                            Grid.Column="0"
                            Margin="1"
                            HorizontalContentAlignment="Left"
                            VerticalContentAlignment="Center"
                            Content=" 품    명" />
                        <TextBox
                            x:Name="txtArticle"
                            Grid.Row="5"
                            Grid.Column="1"
                            Margin="1"
                            VerticalContentAlignment="Center"
                            Style="{DynamicResource essentialTextBox}"
                            Text="{Binding Article}" />


                        <!--  개정내용  -->
                        <Label
                            Grid.Row="6"
                            Grid.Column="0"
                            Margin="1"
                            HorizontalContentAlignment="Left"
                            VerticalContentAlignment="Center"
                            Content="개정내용" />
                        <TextBox
                            x:Name="txtMoldInspectContent"
                            Grid.Row="6"
                            Grid.Column="1"
                            Grid.ColumnSpan="3"
                            Margin="1"
                            VerticalContentAlignment="Center"
                            Background="#fff2d2"
                            Text="{Binding MoldInspectContent}" />

                        <!--  비고  -->
                        <Label
                            Grid.Row="7"
                            Grid.Column="0"
                            Margin="1"
                            HorizontalContentAlignment="Left"
                            VerticalContentAlignment="Center"
                            Content=" 비    고" />
                        <TextBox
                            x:Name="txtComments"
                            Grid.Row="7"
                            Grid.Column="1"
                            Grid.ColumnSpan="3"
                            Margin="1"
                            VerticalContentAlignment="Center"
                            AcceptsReturn="True"
                            Text="{Binding Comments}"
                            TextWrapping="Wrap"
                            VerticalScrollBarVisibility="Visible" />
                    </Grid>

                    <!--  서브그리드  -->
                    <Grid Grid.Row="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="10*" />
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="5*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Button
                                x:Name="btnAddSub"
                                Grid.Column="1"
                                Margin="1"
                                Click="btnAddSub_Click"
                                IsEnabled="False">
                                <StackPanel Orientation="Horizontal">
                                    <Image
                                        VerticalAlignment="Center"
                                        Source="{DynamicResource btiInsert}"
                                        Style="{DynamicResource imgSize}" />
                                    <TextBlock VerticalAlignment="Center" Text="추가" />
                                </StackPanel>
                            </Button>
                            <Button
                                x:Name="btnDeleteSub"
                                Grid.Column="2"
                                Margin="1"
                                Click="btnDeleteSub_Click"
                                IsEnabled="False">
                                <StackPanel Orientation="Horizontal">
                                    <Image
                                        VerticalAlignment="Center"
                                        Source="{DynamicResource btiDelete}"
                                        Style="{DynamicResource imgSize}" />
                                    <TextBlock VerticalAlignment="Center" Text="삭제" />
                                </StackPanel>
                            </Button>
                        </Grid>

                        <DataGrid
                            x:Name="dgdSub"
                            Grid.Row="1"
                            Margin="1"
                            Style="{DynamicResource DataGridSetTwoHeaderEdit}">
                            <DataGrid.Resources>
                                <Style TargetType="DataGridCell">
                                    <EventSetter Event="KeyDown" Handler="DataGird_KeyDown" />
                                    <EventSetter Event="KeyUp" Handler="DatagridIn_TextFocus" />
                                    <EventSetter Event="GotFocus" Handler="DataGridCell_GotFocus" />
                                    <EventSetter Event="MouseUp" Handler="DataGridCell_MouseUp" />
                                </Style>
                            </DataGrid.Resources>
                            <DataGrid.Columns>
                                <DataGridTemplateColumn Header="순번">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Text="{Binding Num}" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="점검항목">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding MoldInspectItemName, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                    <DataGridTemplateColumn.CellEditingTemplate>
                                        <DataTemplate>
                                            <TextBox Loaded="txtBox_Loaded" Text="{Binding MoldInspectItemName, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellEditingTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn x:Name="DataGridTemplateColumn_MachineName" Header="점검내용">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding MoldInspectContent, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                    <DataGridTemplateColumn.CellEditingTemplate>
                                        <DataTemplate>
                                            <TextBox Loaded="txtBox_Loaded" Text="{Binding MoldInspectContent, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellEditingTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="확인방법">
                                    <DataGridTemplateColumn.HeaderTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Vertical">
                                                <TextBlock
                                                    Margin="1"
                                                    HorizontalAlignment="Center"
                                                    Text="확인" />
                                                <TextBlock
                                                    Margin="1"
                                                    HorizontalAlignment="Center"
                                                    Text="방법" />
                                            </StackPanel>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.HeaderTemplate>
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding MoldInspectCheckGbn_Name, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                    <DataGridTemplateColumn.CellEditingTemplate>
                                        <DataTemplate>
                                            <ComboBox
                                                x:Name="cboMoldInspectCheckGbn"
                                                DisplayMemberPath="{Binding code_name}"
                                                Loaded="cboMoldInspectCheckGbn_Loaded"
                                                SelectedValue="{Binding MoldInspectCheckGbn}"
                                                SelectedValuePath="{Binding code_id}"
                                                SelectionChanged="cboMoldInspectCheckGbn_SelectionChanged" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellEditingTemplate>
                                </DataGridTemplateColumn>

                                <DataGridTemplateColumn Header="주기">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding MoldInspectCycleGbn_Name}" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                    <DataGridTemplateColumn.CellEditingTemplate>
                                        <DataTemplate>
                                            <ComboBox
                                                x:Name="cboMoldInspectCycleGbn"
                                                DisplayMemberPath="{Binding code_name}"
                                                Loaded="cboMoldInspectCycleGbn_Loaded"
                                                SelectedValue="{Binding MoldInspectCycleGbn}"
                                                SelectedValuePath="{Binding code_id}"
                                                SelectionChanged="cboMoldInspectCycleGbn_SelectionChanged" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellEditingTemplate>
                                </DataGridTemplateColumn>

                                <DataGridTemplateColumn Header="특정&#10;월/일">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding MoldInspectCycleDate, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                    <DataGridTemplateColumn.CellEditingTemplate>
                                        <DataTemplate>
                                            <TextBox
                                                InputMethod.IsInputMethodEnabled="False"
                                                Loaded="txtBox_Loaded"
                                                PreviewTextInput="txt_PreviewTextInput"
                                                Text="{Binding MoldInspectCycleDate, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellEditingTemplate>
                                </DataGridTemplateColumn>

                                <DataGridTemplateColumn Header="기록&#10;구분">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding MoldInspectRecordGbn_Name}" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                    <DataGridTemplateColumn.CellEditingTemplate>
                                        <DataTemplate>
                                            <ComboBox
                                                x:Name="cboMoldInspectRecordGbn"
                                                DisplayMemberPath="{Binding code_name}"
                                                Loaded="cboMoldInspectRecordGbn_Loaded"
                                                SelectedValue="{Binding MoldInspectRecordGbn}"
                                                SelectedValuePath="{Binding code_id}"
                                                SelectionChanged="cboMoldInspectRecordGbn_SelectionChanged" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellEditingTemplate>
                                </DataGridTemplateColumn>

                                <DataGridTemplateColumn Header="이미지">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Image
                                                MouseLeftButtonDown="img_MouseLeftButtonUp"
                                                Source="{Binding ImageByte, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                                                Tag="{Binding MoldInspectImageFile, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>

                                <DataGridTemplateColumn Header="  ">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Button
                                                VerticalAlignment="Center"
                                                Click="btnUploadAndDelete_Click"
                                                Content="{Binding btnName, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>

                            </DataGrid.Columns>
                        </DataGrid>
                    </Grid>
                </Grid>
            </Grid>

            <TextBlock
                x:Name="tbkIndexCount"
                Grid.Row="1"
                Margin="1"
                Text="▶검색결과 : " />
        </Grid>
    </Grid>
</UserControl>
